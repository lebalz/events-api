{
  "name": "events-api",
  "version": "1.0.0",
  "description": "School Events Planer API",
  "main": "src/app.ts",
  "author": "Balthasar Hofer",
  "license": "MIT",
  "scripts": {
    "build": "npx prisma generate && tsc --build ./tsconfig.json",
    "start": "node -r dotenv/config ./dist/src/app.js",
    "dev": "ts-node -r dotenv/config ./src/app.ts",
    "db_seed": "ts-node -r dotenv/config ./bin/db_seed_prepare.ts && ts-node -r dotenv/config ./bin/db_seed_untis.ts && ts-node -r dotenv/config ./bin/db_seed_files",
    "db_cleanup": "ts-node -r dotenv/config ./bin/db_reset.ts",
    "db_reset": "yarn run db_cleanup && npx prisma migrate reset --force && yarn run db_seed"
  },
  "dependencies": {
    "@azure/msal-node": "^1.14.0",
    "@prisma/client": "5",
    "@socket.io/admin-ui": "^0.5.1",
    "@types/lodash": "^4.14.194",
    "@types/multer": "^1.4.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "exceljs": "^4.3.0",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "ics": "^3.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-azure-ad": "^4.3.4",
    "path": "^0.12.7",
    "read-excel-file": "^5.5.3",
    "socket.io": "^4.6.2",
    "typescript": "^5.2",
    "uuid": "^9.0.0",
    "webuntis": "^2.0.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^20.2.1",
    "@types/passport-azure-ad": "^4.3.1",
    "@types/uuid": "^9.0.1",
    "prisma": "5",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-docs-generator": "^0.8.0",
    "ts-node": "^10.9.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.7",
    "utf-8-validate": "^6.0.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
